def calcParityBit(self, bitD, dic, parity):
		self.parity=parity
		self.bitC=0
		print ('BitD recebido como paramentro em calcPB'),bitD
		for j in dic[1]:
			self.bitC=self.bitC+j
		print ('soma dos bits 1 '),self.bitC
		print len(dic[1])
		return self.calcParity(self.bitC,self.parity)

	def calcParity(self, bit, parity):
		self.parBit=0
		if parity == 1:
			if bit%2==0:
				self.parbit=1
		elif parity == 0:
			if bit%2 !=0:
				self.parBit=1
		return self.parBit
	
	def loadParityBit(self,bitCont,dic,parity):
		self.bits=[]
		print bitCont
		for i in bitCont:
			print ('Posicao i do bitCont '),i
			self.bit = self.calcParityBit(self.i,dic, parity)
			print ('bit de retorno da calcPB'), self.bit	
			self.bits.append(self.bit)
		return self.bits
